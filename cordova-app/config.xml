<?xml version='1.0' encoding='utf-8'?>
<widget id="de.lingualogic.SpeechSampleApp" version="0.5.14" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Speech-SampleApp</name>
    <description>
        Beispiel App fuer Speech-Angular SDK
    </description>
    <author email="leo@lingualogic.de" href="https://lingualogic.de">
        LinguaLogic Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
